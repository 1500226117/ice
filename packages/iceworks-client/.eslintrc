{
  "root": true,
  "parser": "babel-eslint",
  "extends": "eslint-config-airbnb",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "browser": true,
    "mocha": true
  },
  "plugins": ["react", "babel"],
  "rules": {
    "react/jsx-filename-extension": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "arrow-body-style": 0,
    "react/no-array-index-key": 0,
    "react/forbid-prop-types": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0
  }
}
